<Window xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:local="clr-namespace:vkSuite"
		mc:Ignorable="d" x:Class="vkSuite.MainWindow" Title="MainWindow"
		Height="366" Width="245" AllowsTransparency="True" WindowStyle="None"
		ShowInTaskbar="True" Loaded="Window_Loaded" Topmost="True" SizeToContent="Height">
	<Window.Resources>
	</Window.Resources>
	<Window.Background>
		<SolidColorBrush Color="White" Opacity="0" />
	</Window.Background>
	<Border>
		<Border.Background>
			<RadialGradientBrush>
				<GradientStop Color="#FF1E428D" />
				<GradientStop Color="#FF585B60" Offset="1" />
			</RadialGradientBrush>
		</Border.Background>
		<local:MainHolderControl x:Name="main_holder"/>
	</Border>
</Window>
